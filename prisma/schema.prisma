datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  username  String?
  sl_id     String   @unique
  provider  String 
  createdAt DateTime @default(now())
  imageUrl  String?
}
